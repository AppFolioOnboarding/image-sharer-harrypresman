
<h1>Stored Image URLs</h1>
<br/>
<%= link_to 'Create Image', new_image_path %>
<br/>
<br/>
<% @images.each do |image| %>
  <div class='image'>
    <div><b>Image URL: </b><%= image.url %></div>
    <div><%= link_to 'Show image', image_path(image) %></div>
    <div><%= image_tag image.url, width: '400' %></div>
  </div>
<% end %>
